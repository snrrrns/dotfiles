[include]
  path = ~/.gitconfig.local

[alias]
  cm = commit
  co = checkout
  b = branch
  st = status
  df = diff
  tree = log --graph --all --format=\"%x09%C(cyan bold)%an%Creset%x09%C(yellow)%h%Creset %C(magenta reverse)%d%Creset %s\"
  wb = "!f() { git worktree add ../$(basename $(pwd))_$1 $1; }; f"
  wd = worktree remove .
  wl = worktree list
  wp = worktree prune
